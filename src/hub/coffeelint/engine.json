{
    "$schema": "./../.schema/engine.json",
    "name": "coffeelint",
    "description": "CoffeeLint is a style checker that helps keep CoffeeScript code clean and consistent",
    "version": "1.16.0",
    "url": "http://www.coffeelint.org",
    "languages": [
        "coffeescript"
    ],
    "extensions": [
        "*.coffee"
    ],
    "configs": [
        "coffeelint.json"
    ],
    "license": "MIT",
    "areas": [
        "syntax",
        "formatting",
        "smell",
        "complexity",
        "duplication"
    ],
    "requirements": [
        {
            "manager": "system",
            "package": "npm"
        },
        {
            "manager": "npm",
            "package": "coffeelint",
            "version": "1.16.0"
        }
    ],
    "defaults": {
        "--reporter": "file://{schema}/coffeelint/reporter.coffee",
        "": "./"
    },
    "stdin": {
        "--stdin": null
    }
}
