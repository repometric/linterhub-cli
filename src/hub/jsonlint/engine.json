{
    "$schema": "./../.schema/engine.json",
    "name": "jsonlint",
    "description": "A pure JavaScript version of the service provided at jsonlint.com.",
    "url": "https://github.com/zaach/jsonlint",
    "languages": [
        "json"
    ],
    "extensions": [
        "*.json"
    ],
    "license": "MIT",
    "areas": [
        "syntax",
        "formatting"
    ],
    "requirements": [
        {
            "manager": "system",
            "package": "npm"
        },
        {
            "manager": "npm",
            "package": "jsonlint"
        }
    ],
    "postfix": " | node file://{schema}/jsonlint/reporter.js",
    "acceptMask": false,
    "version": {
        "local": "1.6.2",
        "package": "1.6.2"
    },
    "defaults": {
        "": "*",
       // "--quiet": null,
        "--compact": null
    },
    "stdin":
    {
        "available": true,
        "arguments": {
            
        }
    }
}
